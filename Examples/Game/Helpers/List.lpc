function List.New(){
	return []
}

function List.Contains(list, id){
	i=0
	while(i < Lp.Len(list)){
		if(list[i] == id){
			return 1
		}
		i=i+1
	}
	return 0
}

function List.Pos(list, id){
	i=0
	while(i < Lp.Len(list)){
		if(list[i] == id){
			return i
		}
		i=i+1
	}
	return -1
}

function List.Add(list, id){
	list[Lp.Len(list)] = id
	return list
}
