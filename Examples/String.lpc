function Test.ToString(in){
	r =""
	cell = in - Lp.Floor(in)

	while(in > 0){
		a = in / 10
		a = Lp.Floor(a) * 10
		b = in - a
		c = b + 48
		r[Lp.Len(r)]= c
		in = a /10
	}	
	
	rever = ""
	k=0
	rL = Lp.Len(r)
	while(k < rL){
		rever[k] = r[rL -k-1]
		k=k+1
	}
	r=rever

	if (in >0){
		r[Lp.Len(r)]= in + 48
	}

	if(cell > 0)
	{
		r=r+"."
	}

	while(cell < 1 && cell > 0){
		a = cell * 10
		b = Lp.Floor(a) 
		c = b + 48
		r[Lp.Len(r)]=c
		cell = (cell * 10) - b
	}

	return r
}
